  "license": "MIT",
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0"
  }
