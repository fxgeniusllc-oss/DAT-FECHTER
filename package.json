{
  "name": "dex-data-fetcher",
  "version": "1.0.0",
  "description": "DEX Data Fetcher - Aggregate and normalize liquidity pool data from multiple DEXes",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:env": "dotenv -e .env -- jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "clean": "echo 'No build artifacts to clean'",
    "start": "node src/api/index.js",
    "start:dev": "dotenv -e .env -- node src/api/index.js",
    "validate": "node validate-system.js",
    "validate:full": "npm run validate && npm test",
    "test:e2e": "node test-e2e-flow.js"
